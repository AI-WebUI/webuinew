.div-mobile {
	position: absolute;
	background: url(../img/shen6.png);
	background-size: 320px 640px;
	width: 320px;
	height: 640px;
	margin: 4% 0 30px 76%;
	overflow-y: hidden;
	z-index: 55;
	
}

.iframe-mobile {
	width: 285px;
	height: 500px;
	background: #fff;
	margin: 70px 0 0 16px;
	/*border: 1px #333 solid;*/
	
}

.title-p {
	position:absolute;
	top:0px;
	left:250px;
	font-size: 24px;
	color: #fff;
}

.ext{
	
	background:#febe44;
	color: #FFFFFF;
	border-bottom: 2px solid #febe44;
}
.div-code{
	position: absolute;
	width: 61%;
	margin: 200px 0 0 100px;
	/*overflow: scroll;*/
	-webkit-border-radius: 4px;
	-ms-border-radius: 4px;
	-moz-border-radius: 4px;
	
	
}
.div-code pre{
	font-family: "微软雅黑";
	font-size: 16px;
	height:380px;
	color:#333;
	overflow-y: scroll;
	/*font-weight: bold;*/
}
.img-logo{
	display: block;
	position: absolute;
	margin: 13px 0 0 88px;
}
@media screen and (max-width:1043px){.div-code{margin-top:320px;}}
